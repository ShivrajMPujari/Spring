<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">

   <context:annotation-config/>

 	<bean id="student" class="com.bridgIt.springPrax.annotations.Student">
 	<property name="name" value="Jack Long"/>
 	<property name="id" value="101"/>
 	<property name="age" value="23"/>
 	</bean>
 	
 	<bean id="address" class="com.bridgIt.springPrax.annotations.Address">
 	<property name="location" value="Street no-7"/>
 	<property name="city" value="Beijing"/>
 	<property name="state" value="China"/>
 	</bean>

	<bean id="address1" class="com.bridgIt.springPrax.annotations.Address">
 	<property name="location" value="Street no-9"/>
 	<property name="city" value="Shanghai"/>
 	<property name="state" value="China"/>
 	</bean>
</beans>